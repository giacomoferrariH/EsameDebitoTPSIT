<Window x:Class="EsameRecuperoDebito_FerrariGiacomo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EsameRecuperoDebito_FerrariGiacomo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Login" HorizontalAlignment="Center" Height="41" Margin="0,299,0,0" VerticalAlignment="Top" Width="112" Click="Button_Click"/>
        <TextBox HorizontalAlignment="Center" Height="34" Margin="0,105,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="174"/>
        <TextBox HorizontalAlignment="Center" Height="34" Margin="0,144,0,0" TextWrapping="Wrap" Text="Password email" VerticalAlignment="Top" Width="174" TextChanged="TextBox_TextChanged"/>
        <TextBox HorizontalAlignment="Center" Height="34" Margin="0,221,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="174"/>
        <TextBox HorizontalAlignment="Center" Height="34" Margin="0,260,0,0" TextWrapping="Wrap" Text="Password username" VerticalAlignment="Top" Width="174"/>
        <TextBox HorizontalAlignment="Center" Height="34" Margin="0,183,0,0" TextWrapping="Wrap" Text="PIN inviato tramite email" VerticalAlignment="Top" Width="174" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Window>
